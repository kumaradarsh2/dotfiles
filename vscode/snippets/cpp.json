{
	"Competitive Array Template": {
		"prefix": "myarr",
		"body": [
			"#include <bits/stdc++.h>",
			"",
			"class Solution {",
			"public:",
			"    auto function_name(std::vector<int>& arr) {",
			"        // Your logic here",
			"    }",
			"};",
			"",
			"int main() {",
			"    int t;",
			"    std::cin >> t;",
			"    std::cin.ignore();",
			"",
			"    while (t--) {",
			"        std::vector<int> arr;",
			"        std::string input;",
			"        std::getline(std::cin, input);",
			"        std::stringstream ss(input);",
			"        int number;",
			"        while (ss >> number) arr.push_back(number);",
			"",
			"        Solution obj;",
			"        obj.function_name(arr);",
			"",
			"        for (int elem : arr) std::cout << elem << \" \";",
			"        std::cout << \"\\n\";",
			"    }",
			"    return 0;",
			"}" ],
		"description": "Array template"
	},

